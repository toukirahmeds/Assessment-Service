{
   "type": "mysql",
   "host": "localhost",
   "port": 3306,
   "username": "[MYSQL_USERNAME]",
   "password": "[YOUR_MYSQL_USER_PASSWORD]",
   "database": "assessment_service",
   "synchronize": true,
   "logging": false,
   "entities": [
      "src/entity/**/*.ts"
   ],
   "migrations": [
      "src/migration/**/*.ts"
   ],
   "subscribers": [
      "src/subscriber/**/*.ts"
   ],
   "cli": {
      "entitiesDir": "src/entity",
      "migrationsDir": "src/migration",
      "subscribersDir": "src/subscriber"
   },
   "migrationsRun": true
}
